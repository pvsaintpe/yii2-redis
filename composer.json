{
  "name": "pvsaintpe/yii2-redis",
  "description": "Package",
  "keywords": [
    "yii2",
    "redis"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Veselov",
      "email": "pvsaintpe@icloud.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "yiisoft/yii2": "2.0.16.1",
    "ext-redis": ">=2.2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "<6.0",
    "yiisoft/yii2-redis": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "pvsaintpe\\redis\\": ""
    }
  },
  "config": {
    "fxp-asset.installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
    }
  }
}
